// Double Wrapper classes-Autoboxing,AutoUnboxing

class DoubleWrapperclass {
    public static void main(String[] args) {
    System.out.println("AutoBoxing double wrapper class");
    double d1=3.5;
    Double val=d1;   //Double val=Double.valueOf(d1);
     System.out.println(val);
     System.out.println("----AutoUnboxing double wrapper class");
     Double d2=new Double(3.2);
     double val1=d2.doubleValue();
     System.out.println(val1);
       
    }
}
