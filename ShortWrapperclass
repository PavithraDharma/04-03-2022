// Short Wrapper classes-Autoboxing,AutoUnboxing

class ShortWrapperclass {
     public static void main(String[] args) {
     System.out.println("AutoBoxing short wrapper class");
     short s1=225;
     Short val=Short.valueOf(s1);
     System.out.println(val);
     System.out.println("----AutoUnboxing short wrapper class");
     Short s1=-230;
     Short s2=new Short(s1);    //  Integer s2=new Integer(-230);
     short val1=s2.shortValue();
     System.out.println(val1);       
    }
}
